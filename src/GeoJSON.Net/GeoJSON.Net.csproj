<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks Condition="'$(LibraryFrameworks)'==''">net35;netstandard10</TargetFrameworks>
    <TargetFrameworks Condition="'$(LibraryFrameworks)'!=''">$(LibraryFrameworks)</TargetFrameworks>
    <VersionPrefix>1.0.1</VersionPrefix>
    <VersionSuffix>alpha</VersionSuffix>
    <Authors>Matt Hunt, Joerg Battermann</Authors>
    <Company>GeoJSON.Net</Company>
    <AssemblyTitle>GeoJSON.Net</AssemblyTitle>
    <Description>.Net library for GeoJSON types and corresponding Json.Net (de)serializers.</Description>
    <Copyright>Copyright © Matt Hunt, Joerg Battermann 2017</Copyright>
    <Summary>.Net library for GeoJSON types and corresponding Json.Net (de)serializers.</Summary>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageId>GeoJSON.Net</PackageId>
    <PackageTags>geojson geo json geolocation</PackageTags>
    <PackageProjectUrl>https://github.com/GeoJSON-Net/GeoJSON.Net</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/GeoJSON-Net/GeoJSON.Net/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/GeoJSON-Net/GeoJSON.Net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RootNamespace>GeoJSON.Net</RootNamespace>
    <AssemblyName>GeoJSON.Net</AssemblyName>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="newtonsoft.json" Version="10.0.2" />
  </ItemGroup>
</Project>